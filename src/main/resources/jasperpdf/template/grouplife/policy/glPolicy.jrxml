				</textElement>
				<text><![CDATA[1.CONTRACT ]]></text>
			</staticText>
			<staticText>
				<reportElement x="30" y="190" width="170" height="20" uuid="e5ed1995-0617-4574-ae33-6e2fff4be15f"/>
				<textElement textAlignment="Justified">
